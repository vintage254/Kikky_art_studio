{
  "buildCommand": "node --import=node:module install-deps.mjs && npm run build",
  "installCommand": "npm install --no-optional",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "env": {
    "VERCEL": "true",
    "NODE_ENV": "production",
    "SHARP_IGNORE_GLOBAL_LIBVIPS": "1"
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1",
      "SHARP_IGNORE_GLOBAL_LIBVIPS": "1"
    }
  }
}